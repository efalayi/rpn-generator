{
  "name": "rpn-generator",
  "version": "1.0.0",
  "description": "A random phone number generator app",
  "main": "server.js",
  "repository": "git@github.com:efalayi/rpn-generator.git",
  "author": "Esther Falayi <esther.falayi@andela.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:config build:server build:client",
    "build:client": "./node_modules/.bin/webpack --config webpack.prod.config.babel.js",
    "build:config": "babel --root-mode upward config -d dist/config",
    "build:server": "babel --root-mode upward server -d dist/server",
    "build:dev:server": "babel-node server/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint './**/*.js'",
    "prebuild": "rimraf dist && mkdir dist",
    "prestart": "npm run build",
    "start": "npm run start:api",
    "start:api": "node dist/server/server.js",
    "start:api:dev": "nodemon",
    "start:dev": "npm-run-all --parallel start:api:dev start:react:dev",
    "start:react:dev": "webpack-dev-server --config webpack.dev.config.babel.js --watch",
    "test": "npm run test:client --updateSnapshot",
    "test:client": "jest --config jest.config.js --coverage"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "2.4.0",
    "chance": "^1.0.18",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "image-webpack-loader": "^4.5.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "jsdom": "^13.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "react-test-renderer": "^16.6.3",
    "rosie": "^2.0.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
